{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "description": "",
    "type": "object",
    "properties": {
        "mes_inicial": {
            "type": "string",
            "minLength": 1
        },
        "mes_final": {
            "type": "string",
            "minLength": 1
        },
        "cabecalho_lancamentos": {
            "type": "array",
            "uniqueItems": true,
            "minItems": 1,
            "items": {
                "required": [
                    "descricao_lancamento",
                    "sinal_valor_lancamento",
                    "valor_lancamento"
                ],
                "properties": {
                    "descricao_lancamento": {
                        "type": "string",
                        "minLength": 1
                    },
                    "sinal_valor_lancamento": {
                        "type": "string",
                        "minLength": 1
                    },
                    "valor_lancamento": {
                        "type": "number"
                    }
                }
            }
        },
        "lancamentos": {
            "type": "array",
            "uniqueItems": true,
            "minItems": 1,
            "items": {
                "required": [
                    "data_vencimento",
                    "sinal_total_mes",
                    "total_mes",
                    "total_quantidade_lancamentos_mes"
                ],
                "properties": {
                    "data_vencimento": {
                        "type": "string",
                        "minLength": 1
                    },
                    "relacao_compras_parceladas": {
                        "type": "array",
                        "uniqueItems": true,
                        "minItems": 1,
                        "items": {
                            "required": [
                                "nome_titular",
                                "numero_cartao",
                                "sinal_total_lancamentos_cartao",
                                "total_lancamentos_cartao",
                                "total_quantidade_lancamentos_cartao"
                            ],
                            "properties": {
                                "nome_titular": {
                                    "type": "string",
                                    "minLength": 1
                                },
                                "numero_cartao": {
                                    "type": "string",
                                    "minLength": 1
                                },
                                "detalhe_lancamentos": {
                                    "type": "array",
                                    "uniqueItems": true,
                                    "minItems": 1,
                                    "items": {
                                        "required": [
                                            "data_movimentacao",
                                            "descricao_lancamento",
                                            "sinal_valor_lancamento",
                                            "valor_lancamento"
                                        ],
                                        "properties": {
                                            "data_movimentacao": {
                                                "type": "string",
                                                "minLength": 1
                                            },
                                            "descricao_lancamento": {
                                                "type": "string",
                                                "minLength": 1
                                            },
                                            "sinal_valor_lancamento": {
                                                "type": "string",
                                                "minLength": 1
                                            },
                                            "valor_lancamento": {
                                                "type": "number"
                                            }
                                        }
                                    }
                                },
                                "sinal_total_lancamentos_cartao": {
                                    "type": "string",
                                    "minLength": 1
                                },
                                "total_lancamentos_cartao": {
                                    "type": "number"
                                },
                                "total_quantidade_lancamentos_cartao": {
                                    "type": "number"
                                }
                            }
                        }
                    },
                    "sinal_total_mes": {
                        "type": "string",
                        "minLength": 1
                    },
                    "total_mes": {
                        "type": "number"
                    },
                    "total_quantidade_lancamentos_mes": {
                        "type": "number"
                    }
                }
            }
        }
    },
    "required": [
        "mes_inicial",
        "mes_final",
        "cabecalho_lancamentos",
        "lancamentos"
    ]
}